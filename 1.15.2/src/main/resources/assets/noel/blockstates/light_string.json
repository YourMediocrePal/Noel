{
    "multipart": [
        {   "apply": { "model": "noel:light_string_post" }},
        {   "when": { "north": true },
            "apply": { "model": "noel:light_string_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "noel:light_string_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "noel:light_string_side_alt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "noel:light_string_side_alt", "y": 90 }
        },
        {   "when": { "north": false },
            "apply": { "model": "noel:light_string_noside" }
        },
        {   "when": { "east": false },
            "apply": { "model": "noel:light_string_noside_alt" }
        },
        {   "when": { "south": false },
            "apply": { "model": "noel:light_string_noside_alt", "y": 90 }
        },
        {   "when": { "west": false },
            "apply": { "model": "noel:light_string_noside", "y": 270 }
        },
        {
            "when": { "north": false, "east": false, "south": false, "west": false },
            "apply": { "model": "noel:light_string_post_noside" }
        }
    ]
}
